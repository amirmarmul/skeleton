{
    "name": "kcdev/:package_name",
    "description": ":package_description",
    "keywords": [
        "kcdev",
        ":package_name"
    ],
    "homepage": "https://github.com/spatie/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "homepage": "https://kcdev.id",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Kcdev\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kcdev\\Skeleton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kcdev\\Skeleton\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "Kcdev\\Skeleton\\Facades\\SkeletonFacade"
            }
        }
    }
}
